{
  "env": {
    "browser": 1,
    "node": 1,
    "es6": 1
  },
  "plugins": [ "react" ],
  "parser": "babel-eslint",
  "extends": [ "eslint:recommended", "plugin:react/recommended" ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}