{
  "hosting": {
    "public": "build"
    , "headers": [ {
      "source" : "**"
      , "headers" : [
        {
          "key" : "Access-Control-Allow-Origin"
          , "value" : "*"
        }
      ]
    } ]
    , "ignore": []
    , "rewrites": [
        {
          "source": "**"
          , "destination": "/index.html"
        }
      ]
  }

}
